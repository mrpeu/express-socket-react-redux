  "main": "server/main.js",
  "scripts": {
    "dev-server": "nodemon --watch server --exec babel-node server/main.js",
    "dev-client": "webpack --watch client --config client/webpack.config.js",
    "client": "webpack --config client/webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ansi-styles": "^2.2.1",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-watch": "^2.0.2",
    "chalk": "^1.1.3",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.13.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.11.1",
    "react": "^15.0.1",
    "react-bootstrap": "^0.29.2",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "remarkable": "^1.6.2",
    "socket.io": "^1.4.5",
    "webpack": "^1.13.0",
    "webpack-livereload-plugin": "^0.8.1"
