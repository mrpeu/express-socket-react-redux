  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.6.5",
    "babel-plugin-transform-react-constant-elements": "^6.0.14",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-2": "^6.5.0",
    "babel-watch": "^1.0.2",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-livereload-plugin": "^0.7.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "no-console": 0,
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        1,
        "only-multiline"
      ],
      "no-shadow": [
        2,
        {
          "allow": [
            "state"
          ]
        }
      ],
      "new-cap": 0,
      "curly": [
        2,
        "multi-line",
        "consistent"
      ],
      "space-in-parens": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "computed-property-spacing": [
        2,
        "always"
      ],
      "no-else-return": 0,
      "no-param-reassign": 0,
      "no-unused-vars": 0,
      "no-trailing-spaces": 0
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "airbnb"
  }
}
